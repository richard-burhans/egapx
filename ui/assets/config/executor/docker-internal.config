    PATH = "/img/gp/bin/:/img/gp/bin/third-party/:/img/gp/bin/third-party/sratoolkit.3.0.7-ubuntu64/bin/:/img/gp/bin/third-party/STAR/bin/Linux_x86_64:\$PATH"
